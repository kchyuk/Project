###### 라이브러리 호출 ######
import Automatic_def # 숫자를 입력하면 억원 단위로 출력해주는 함수 모음
from Automatic_def import *

import pandas as pd
from pandas import DataFrame
from pandas.tseries.offsets import DateOffset

import numpy as np
import xlwings as xw

import shutil
import win32com.client as win32

import pyautogui

import calendar
import datetime
from datetime import datetime
import time

##### 엑셀 Input / 한글 Input & Output 지정 ##### 

######## 엑셀 Input ########
book_101 = xw.Book('2022년02월말운용현황.xlsx') # 엑셀 파일 열기 ★★★사용자 지정★★★
sh101 = book_101.sheets('값최종') # 
sh11p = book_101.sheets('11p') # 
sh12p = book_101.sheets('12p') # 
sh13p = book_101.sheets('13p') # 
sh16p = book_101.sheets('16p') # 

######## 한글 Input & Output ########
shutil.copyfile(r"./2022년01월운용현황.hwp", r"./2022년02월운용현황.hwp") #★★★사용자 지정★★★
hwp = win32.gencache.EnsureDispatch("HWPFrame.HwpObject")
# hwp.RegisterModule("FilePathChecKDLL", "FilePathCheckerModule") # 보안모듈 관련 부분은 추후 재시도할 것 
hwp.Open(r"C:\Users\이영화\Desktop\python\2_2.FinancialManagement\`22년02월말\2022년02월운용현황.hwp") #★★★사용자 지정★★★

# hwp.GetFieldList() # hwp.GetFieldList(1)
# len([i for i in hwp.GetFieldList().split('\x02')]) # 누름틀을 확인합니다.

##### page0 (누름틀 3개) #####
hwp.PutFieldText('page0_1', sh101.range('C2').value)
hwp.PutFieldText('page0_2', sh101.range('C2').value[:4])
hwp.PutFieldText('page0_3', int(sh101.range('C2').value.split(" ")[1].replace("월말",""))+1) #★연도 바뀔 때 주의

n월말 = hwp.GetFieldText('page0_1').split(" ")[1]

##### page1 (누름틀 62개 / 누적 65개) #####
for a in range(0,9): #page1의 자금조달현황에서 C열값을 B열로 옮김
     hwp.PutFieldText(f"page1_{a+21}",hwp.GetFieldText(f"page1_{a+30}")) 
        
hwp.PutFieldText('page1_30',f'22.{n월말}') #C열 채우기 ★연도 바뀔 때 변환
for a in range(0,8): 
    hwp.PutFieldText(f'page1_{a+31}',format(round(sh101.range(f'N{a+3}').value),','))

for a in range(0,8): #C-B
    hwp.PutFieldText(f'page1_{a+39}',format((int(hwp.GetFieldText(f'page1_{a+31}').replace(',',''))-int(hwp.GetFieldText(f'page1_{a+22}').replace(',',''))),','))

for a in range(0,8): #C-A 대비
    hwp.PutFieldText(f'page1_{a+47}',format((int(hwp.GetFieldText(f'page1_{a+31}').replace(',',''))-int(hwp.GetFieldText(f'page1_{a+13}').replace(',',''))),','))

for a in range(0,8): #C-A 성장률
    hwp.PutFieldText(f'page1_{a+55}', round((int(hwp.GetFieldText(f'page1_{a+47}').replace(',',''))/int(hwp.GetFieldText(f'page1_{a+13}').replace(',','')))*100,1))

hwp.PutFieldText('page1_4',hwp.GetFieldText('page1_5')) # 총조달자금
hwp.PutFieldText('page1_5',hwp.GetFieldText('page1_6'))
hwp.PutFieldText('page1_6',hwp.GetFieldText('page1_21').split(".")[1].replace("말","")+' '+hwp.GetFieldText('page1_2')) 
hwp.PutFieldText('page1_1', transHangeul(hwp.GetFieldText('page1_38')))
hwp.PutFieldText('page1_2', transHangeulUpDown(hwp.GetFieldText('page1_54')))
hwp.PutFieldText('page1_3', transHangeulUpDown(hwp.GetFieldText('page1_46')))
hwp.PutFieldText('page1_10',hwp.GetFieldText('page1_11')) # 예수금
hwp.PutFieldText('page1_11',hwp.GetFieldText('page1_12'))
hwp.PutFieldText('page1_12',hwp.GetFieldText('page1_21').split(".")[1].replace("말","")+' '+hwp.GetFieldText('page1_8'))
hwp.PutFieldText('page1_7', transHangeul(hwp.GetFieldText('page1_34')))
hwp.PutFieldText('page1_8', transHangeulUpDown(hwp.GetFieldText('page1_50')))
hwp.PutFieldText('page1_9', transHangeulUpDown(hwp.GetFieldText('page1_42')))

##### page2 (누름틀 124개 / 누적 189개) #####
for a in range(0,20): #page2의 자금조달현황에서 C열값을 B열로 옮김
     hwp.PutFieldText(f"page2_{a+28}",hwp.GetFieldText(f"page2_{a+48}")) 

hwp.PutFieldText('page2_48',f'22.{n월말}') #C열 채우기 ★연도 바뀔 때 주의

for a in range(0,19): 
    hwp.PutFieldText(f'page2_{a+49}',format(round(sh101.range(f'Q{a+3}').value),','))

for a in range(0,19): #C-B
    hwp.PutFieldText(f'page2_{a+68}',format((int(hwp.GetFieldText(f'page2_{a+49}').replace(',',''))-int(hwp.GetFieldText(f'page2_{a+29}').replace(',',''))),','))

for a in range(0,19): #C-B
    hwp.PutFieldText(f'page2_{a+87}',format((int(hwp.GetFieldText(f'page2_{a+49}').replace(',',''))-int(hwp.GetFieldText(f'page2_{a+9}').replace(',',''))),','))

for a in range(0,19): #C-A 성장률
    hwp.PutFieldText(f'page2_{a+106}', round((int(hwp.GetFieldText(f'page2_{a+87}').replace(',',''))/int(hwp.GetFieldText(f'page2_{a+9}').replace(',','')))*100,1))

hwp.PutFieldText('page2_1',transHangeul(hwp.GetFieldText('page2_63'))) # 수익성자산
hwp.PutFieldText('page2_2',transHangeulUpDown(hwp.GetFieldText('page2_101')))
hwp.PutFieldText('page2_3',transHangeulUpDown(hwp.GetFieldText('page2_90')))
hwp.PutFieldText('page2_4',transHangeulUpDown(hwp.GetFieldText('page2_94')))
hwp.PutFieldText('page2_5',transHangeulUpDown(hwp.GetFieldText('page2_100')))
hwp.PutFieldText('page2_6',transHangeulUpDown(hwp.GetFieldText('page2_82')))
hwp.PutFieldText('page2_7',transHangeul(hwp.GetFieldText('page2_66'))) # 수익성자산 외
hwp.PutFieldText('page2_8',transHangeul(hwp.GetFieldText('page2_64')))

### page3 (누름틀 112개 / 누적 301개) 

hwp.PutFieldText('page3_40',f'22.{n월말}') #B열 채우기 ★연도 바뀔 때 주의

for a in range(0,17): 
    hwp.PutFieldText(f'page3_{a+41}',format(round(sh101.range(f'Q{a+3}').value),','))

hwp.PutFieldText('page3_58',format(round(sh101.range('Q21').value),','))


for a in range(0,18): #(나)열 채우기
    hwp.PutFieldText(f'page3_{a+59}', round((int(hwp.GetFieldText(f'page3_{a+41}').replace(',',''))/int(hwp.GetFieldText('page3_58').replace(',','')))*100,1))

for a in range(0,18): #(B/A) 채우기 # 0으로 값을 나누게 될 오류에 대비
    try:
        hwp.PutFieldText(f'page3_{a+77}',round((int(hwp.GetFieldText(f'page3_{a+41}').replace(",",''))/int(hwp.GetFieldText(f'page3_{a+4}').replace(",",'')))*100,1))
    except:  
        hwp.PutFieldText(f'page3_{a+77}',0)
    else:    
        hwp.PutFieldText(f'page3_{a+77}',round((int(hwp.GetFieldText(f'page3_{a+41}').replace(",",''))/int(hwp.GetFieldText(f'page3_{a+4}').replace(",",'')))*100,1))

for a in range(0,18): #(나)-(가) 채우기
    hwp.PutFieldText(f'page3_{a+95}', round(float(hwp.GetFieldText(f'page3_{a+59}'))-float(hwp.GetFieldText(f'page3_{a+22}')),1))

hwp.PutFieldText('page3_1',transHangeul(hwp.GetFieldText('page3_55'))) # 수익성자산
hwp.PutFieldText('page3_3', hwp.GetFieldText('page3_91')) # 달성률

### page4 (누름틀 94개 / 누적 395개)

hwp.PutFieldText('page4_34',n월말) # (B)열 채우기

for a in range(0,15):
    hwp.PutFieldText(f'page4_{a+35}',format(round(sh101.range(f'T{a+3}').value),','))

for a in range(0,15): #(나)열 채우기
    hwp.PutFieldText(f'page4_{a+50}', round(int(hwp.GetFieldText(f'page4_{a+35}').replace(",",""))/int(hwp.GetFieldText('page4_49').replace(",",""))*100,1))

for a in range(0,15): #(B/A) 채우기 # 0으로 값을 나누게 될 오류에 대비
    try :
        hwp.PutFieldText(f'page4_{a+65}',round((int(hwp.GetFieldText(f'page4_{a+35}').replace(",",''))/int(hwp.GetFieldText(f'page4_{a+4}').replace(",",'')))*100,1))
    except:
        hwp.PutFieldText(f'page4_{a+65}',0)
    else:
        hwp.PutFieldText(f'page4_{a+65}',round((int(hwp.GetFieldText(f'page4_{a+35}').replace(",",''))/int(hwp.GetFieldText(f'page4_{a+4}').replace(",",'')))*100,1))
        
for a in range(0,15): #(나)-(가) 채우기
    hwp.PutFieldText(f'page4_{a+80}', round(float(hwp.GetFieldText(f'page4_{a+50}'))-float(hwp.GetFieldText(f'page4_{a+19}')),1))

hwp.PutFieldText('page4_1',transHangeul(hwp.GetFieldText('page4_49'))) #연간 수익성자산 신규운용
hwp.PutFieldText('page4_3', hwp.GetFieldText('page4_79')) # 달성률

### page5 (누름틀 94개 / 누적 489개)

hwp.PutFieldText('page5_34',n월말) # (B)열 채우기

for a in range(0,15): #(A)열 채우기
    hwp.PutFieldText(f'page5_{a+4}',format(round(sh101.range(f'W{a+3}').value),','))

for a in range(0,15): #(가)열 채우기
    hwp.PutFieldText(f'page5_{a+19}', round((int(hwp.GetFieldText(f'page5_{a+4}').replace(",",""))/int(hwp.GetFieldText('page5_18').replace(",","")))*100,1))

for a in range(0,15): #(B)열 채우기
    hwp.PutFieldText(f'page5_{a+35}', format(round(sh101.range(f'X{a+3}').value),','))

for a in range(0,15): #(나)열 채우기
    hwp.PutFieldText(f'page5_{a+50}', round((int(hwp.GetFieldText(f'page5_{a+35}').replace(",",""))/int(hwp.GetFieldText('page5_49').replace(",","")))*100,1))

for a in range(0,15): #(B/A)열 채우기 # 0으로 값을 나누게 될 오류를 대비
    try :    
        hwp.PutFieldText(f'page5_{a+65}', round((int(hwp.GetFieldText(f'page5_{a+35}').replace(",",""))/int(hwp.GetFieldText(f'page5_{a+4}').replace(",","")))*100,1))
    
    except : # ZeroDivisionError 
        hwp.PutFieldText(f'page5_{a+65}', 0)
        
    else :
        hwp.PutFieldText(f'page5_{a+65}', round((int(hwp.GetFieldText(f'page5_{a+35}').replace(",",""))/int(hwp.GetFieldText(f'page5_{a+4}').replace(",","")))*100,1))

for a in range(0,15): #(나)-(가) 채우기
    hwp.PutFieldText(f'page5_{a+80}',round(float(hwp.GetFieldText(f'page5_{a+50}'))-float(hwp.GetFieldText(f'page5_{a+19}')),1))

hwp.PutFieldText('page5_1',n월말.replace("말","")) #(n월 중)
hwp.PutFieldText('page5_2',transHangeul(hwp.GetFieldText('page5_49'))) #수익성자산 신규운용
hwp.PutFieldText('page5_3',hwp.GetFieldText('page5_79')) #계획대비 운용 비중

### page6 (누름틀 110개 /  누적 599개)

for a in range(0,15): #평잔 채우기
    hwp.PutFieldText(f'page6_{a+4}',format(round(sh101.range(f'AA{a+3}').value),','))

for a in range(0,15): #운용손익 채우기
    hwp.PutFieldText(f'page6_{a+19}',format(round(sh101.range(f'AB{a+3}').value),','))

for a in range(0,16): #N-1월 말 열을 왼쪽으로 
     hwp.PutFieldText(f"page6_{a+49}",hwp.GetFieldText(f"page6_{a+65}")) 

hwp.PutFieldText('page6_65',n월말)

for a in range(0,15): #N월말 채우기
     hwp.PutFieldText(f"page6_{a+66}",round(sh101.range(f'AC{a+3}').value,2)) 

for a in range(0,15): #전년말 채우기
    hwp.PutFieldText(f'page6_{a+81}',round(float(hwp.GetFieldText(f'page6_{a+66}'))-float(hwp.GetFieldText(f'page6_{a+34}')),2))

for a in range(0,15): #전월말 채우기
    hwp.PutFieldText(f'page6_{a+96}',round(float(hwp.GetFieldText(f'page6_{a+66}'))-float(hwp.GetFieldText(f'page6_{a+50}')),2))

hwp.PutFieldText('page6_1',hwp.GetFieldText('page6_80')) #수익성자산 운용수익률
hwp.PutFieldText('page6_2', upanddown(hwp.GetFieldText('page6_95'))) #전년말 대비
hwp.PutFieldText('page6_3', upanddown(hwp.GetFieldText('page6_110'))) #전월말 대비

### page7 (누름틀 110개 /  누적 709개)

for a in range(0,15): #평잔 채우기
    hwp.PutFieldText(f'page7_{a+4}',format(round(sh101.range(f'AF{a+3}').value),','))

for a in range(0,15): #운용손익 채우기
     hwp.PutFieldText(f'page7_{a+19}',format(round(sh101.range(f'AG{a+3}').value),','))

for a in range(0,16): #N-1월 말 열을 왼쪽으로 
      hwp.PutFieldText(f"page7_{a+49}",hwp.GetFieldText(f"page7_{a+65}")) 

hwp.PutFieldText('page7_65',n월말)

for a in range(0,15): #N월말 채우기
      hwp.PutFieldText(f"page7_{a+66}","{:.2f}".format(sh101.range(f'AH{a+3}').value)) 

for a in range(0,15): #전년말 채우기
     hwp.PutFieldText(f'page7_{a+81}',"{:.2f}".format(float(hwp.GetFieldText(f'page7_{a+66}'))-float(hwp.GetFieldText(f'page7_{a+34}'))))

for a in range(0,15): #전월말 채우기
     hwp.PutFieldText(f'page7_{a+96}',"{:.2f}".format(float(hwp.GetFieldText(f'page7_{a+66}'))-float(hwp.GetFieldText(f'page7_{a+50}'))))

hwp.PutFieldText('page7_1',hwp.GetFieldText('page7_80')) #수익성자산 운용수익률
hwp.PutFieldText('page7_2', upanddown(hwp.GetFieldText('page7_95'))) #전년말 대비
hwp.PutFieldText('page7_3', upanddown(hwp.GetFieldText('page7_110'))) #전월말 대비

### page8 (누름틀 116개 / 누적 825개)

for a in range(0,18): #평잔 채우기
    hwp.PutFieldText(f'page8_{a+9}',format(round(sh101.range(f'AK{a+3}').value),','))

for a in range(0,18): #손익 채우기
    hwp.PutFieldText(f'page8_{a+27}',format(round(sh101.range(f'AL{a+3}').value),','))

for a in range(0,18): #수익률 채우기
    hwp.PutFieldText(f'page8_{a+45}',"{:.2f}".format(sh101.range(f'AM{a+3}').value))

for a in range(0,18): #평가손익누계 채우기
    hwp.PutFieldText(f'page8_{a+63}',format(round(sh101.range(f'AN{a+3}').value),','))

for a in range(0,18): #평가손익증감 채우기
    hwp.PutFieldText(f'page8_{a+81}',format(round(sh101.range(f'AO{a+3}').value),','))

for a in range(0,18): #운용 수익률 채우기
    hwp.PutFieldText(f'page8_{a+99}',"{:.2f}".format(sh101.range(f'AP{a+3}').value))

hwp.PutFieldText('page8_3',hwp.GetFieldText('page8_4')) #○결산수익률 한 칸 씩 왼쪽으로
hwp.PutFieldText('page8_4',hwp.GetFieldText('page8_5')) #○결산수익률 한 칸 씩 왼쪽으로
hwp.PutFieldText('page8_5','('+str(int(n월말.replace('월말',''))-1)+'월) '+hwp.GetFieldText('page8_1')) #○ 맨 우측 결산수익률#★연도 바뀔 때 주의
hwp.PutFieldText('page8_1',"{:.2f}".format(float(hwp.GetFieldText('page8_62')))) #□결산수익률

hwp.PutFieldText('page8_6',hwp.GetFieldText('page8_7')) #○운용수익률 한 칸 씩 왼쪽으로
hwp.PutFieldText('page8_7',hwp.GetFieldText('page8_8')) #○운용수익률 한 칸 씩 왼쪽으로
hwp.PutFieldText('page8_8','('+str(int(n월말.replace('월말',''))-1)+'월) '+hwp.GetFieldText('page8_2')) #○ 맨 우측 운용수익률#★연도 바뀔 때 주의
hwp.PutFieldText('page8_2',"{:.2f}".format(float(hwp.GetFieldText('page8_116')))) #□운용수익률

