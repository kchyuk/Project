hwp.PutFieldText('page24_32',"{:.1f}".format(sh101.range('EP5').value))

hwp.PutFieldText('page24_1', transHangeul(hwp.GetFieldText('page24_18'))) #부동산투자 잔액
hwp.PutFieldText('page24_2', transHangeulUpDown(hwp.GetFieldText('page24_21'))) #전년말 대비
hwp.PutFieldText('page24_3', transHangeulUpDown(hwp.GetFieldText('page24_24'))) #전월말 대비
hwp.PutFieldText('page24_4',n월말.replace("월말",""))
hwp.PutFieldText('page24_5', transHangeul(hwp.GetFieldText('page24_30')))

hwp.PutFieldText('page24_6', transHangeul(hwp.GetFieldText('page24_28'))) #해외사모 신규
hwp.PutFieldText('page24_7', transHangeul(hwp.GetFieldText('page24_29'))) #수익증권 신규

#손익현황
for a in range(0,3): #평잔 채우기
    hwp.PutFieldText(f'page24_{a+36}',format(round(sh101.range(f'EL{a+8}').value),','))

for a in range(0,3): #손익 채우기
    hwp.PutFieldText(f'page24_{a+39}',format(round(sh101.range(f'EM{a+8}').value),','))

for a in range(0,3): #수익률 채우기
    hwp.PutFieldText(f'page24_{a+42}',"{:.2f}".format(sh101.range(f'EN{a+8}').value))

for a in range(0,3): #전년말 채우기
    hwp.PutFieldText(f'page24_{a+45}',format(round(sh101.range(f'EO{a+8}').value),','))

hwp.PutFieldText('page24_48',n월말) #n월말

for a in range(0,3): #n월말 채우기
    hwp.PutFieldText(f'page24_{a+49}',format(round(sh101.range(f'EP{a+8}').value),','))

for a in range(0,3): #증감 채우기
    hwp.PutFieldText(f'page24_{a+52}',format(round(sh101.range(f'EQ{a+8}').value),','))

for a in range(0,3): #운용 수익률 채우기
    hwp.PutFieldText(f'page24_{a+55}',"{:.2f}".format(sh101.range(f'ER{a+8}').value))

hwp.PutFieldText('page24_33', hwp.GetFieldText('page24_44')) #해외투자 결산수익률
hwp.PutFieldText('page24_34', transHangeul(hwp.GetFieldText('page24_41'))) #결산손익
hwp.PutFieldText('page24_35', hwp.GetFieldText('page24_57')) #운용수익률



